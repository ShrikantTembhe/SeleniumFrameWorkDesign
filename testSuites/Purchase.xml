<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<!--To run the test parallely use below syntax as:-->
<suite parallel="tests" name="Suite">

	<groups>
		<run>
			<include name="Purchase"></include>
		</run>
	</groups>

	<test thread-count="5" name="SubmitOrderTest">
		<classes>
			<class
				name="rahulshettyacademy.SeleniumFrameworkDesign.SubmitOrderTest" />
		</classes>
	</test> <!--
	Test -->
	<test thread-count="5" name="ErrorValidation">
		<classes>
			<class
				name="rahulshettyacademy.SeleniumFrameworkDesign.ErrorValidation"></class>
		</classes>
	</test>
</suite> <!--
Suite -->

